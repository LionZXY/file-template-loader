{
  "name": "Domain",
  "moduleName": "${PROJECT_NAME_LOWER_KEBAB_CASE}-domain-${MODEL_NAME_LOWER_CASE}",
  "sourceSets": [
    {
      "directory": "src/main/kotlin",
      "templates": [
        {
          "template": "Model.kt",
          "fileName": "${MODEL_NAME}Model"
        },
        {
          "template": "ModelImpl.kt",
          "fileName": "${MODEL_NAME}ModelImpl"
        },
        {
          "template": "ModelModule.kt",
          "fileName": "Domain${MODEL_NAME}Module",
          "directory": "di"
        }
      ]
    },
    {
      "templates": [
        {
          "template": "DomainModuleBuild.gradle",
          "fileName": "build"
        },
        {
          "template": "GitIgnore.txt",
          "fileName": ".gitignore"
        }
      ]
    },
    {
      "directory": "src/main",
      "templates": [
        {
          "template": "AndroidManifestModule.xml",
          "fileName": "AndroidManifest"
        }
      ]
    }
  ],
  "injectors": [
    {
      "text": ",\n':${MODULE_NAME}'",
      "pathName": "${SETTING_FILE_PATH}",
      "selector": "Literal"
    }
  ]
}
