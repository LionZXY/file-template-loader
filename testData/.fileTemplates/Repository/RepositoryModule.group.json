{
  "templates": [
    {
      "textFrom": "RepositoryGroup.group.json",
      "customProps": [
        {
          "name": "FILE_DIRECTORY",
          "text": "src/main/kotlin"
        }
      ]
    },
    {
      "textFrom": "RepositoryModuleBuild.gradle.ft",
      "customProps": [
        {
          "name": "FILE_NAME",
          "text": "build.gradle"
        }
      ]
    },
    {
      "textFrom": ".gitignore.ft",
      "customProps": [
        {
          "name": "FILE_NAME",
          "text": ".gitignore"
        }
      ]
    },
    {
      "textFrom": "AndroidManifest.xml.ft",
      "customProps": [
        {
          "name": "FILE_NAME",
          "text": "AndroidManifest.xml"
        },
        {
          "name": "FILE_DIRECTORY",
          "text": "src/main"
        }
      ]
    }
  ],
  "customProps": [
    {
      "name": "PACKAGE_NAME",
      "text": "${PACKAGE_NAME}.${MODULE_NAME_DOT_CASE}"
    },
    {
      "name": "FILE_DIRECTORY",
      "text": "./${MODULE_NAME}"
    },
    {
      "name": "MODULE_NAME",
      "text": "epic-repository"
    }
  ],
  "injectors": [
    {
      "text": ",\n':${MODULE_NAME}'",
      "pathName": "${SETTING_FILE_PATH}",
      "selector": "Literal"
    }
  ]
}
