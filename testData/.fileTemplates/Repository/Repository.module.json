{
  "name": "Repository",
  "moduleName": "${PROJECT_NAME_LOWER_KEBAB_CASE}-repositories-${REPOSITORY_NAME_LOWER_CASE}",
  "sourceSets": [
    {
      "directory": "src/main/kotlin",
      "templates": [
        {
          "template": "Repository.kt",
          "fileName": "${REPOSITORY_NAME_UPPER_CAMEL_CASE}Repository"
        },
        {
          "template": "RepositoryImpl.kt",
          "fileName": "${REPOSITORY_NAME_UPPER_CAMEL_CASE}RepositoryImpl"
        },
        {
          "template": "RepositoriesBindings.kt",
          "fileName": "Repository${REPOSITORY_NAME_UPPER_CAMEL_CASE}Bindings",
          "directory": "di"
        }
      ]
    },
    {
      "templates": [
        {
          "template": "RepositoryModuleBuild.gradle",
          "fileName": "build"
        },
        {
          "template": "GitIgnore.txt",
          "fileName": ".gitignore"
        }
      ]
    },
    {
      "directory": "src/main",
      "templates": [
        {
          "template": "AndroidManifestModule.xml",
          "fileName": "AndroidManifest"
        }
      ]
    }
  ],
  "injectors": [
    {
      "text": ",\n':${MODULE_NAME}'",
      "pathName": "${SETTING_FILE_PATH}",
      "selector": "Literal"
    }
  ]
}
